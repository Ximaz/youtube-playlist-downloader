{
  "name": "sandbox",
  "type": "module",
  "private": true,
  "scripts": {
    "build:cli": "bun build:cli:video && bun build:cli:videos && bun build:cli:playlist",
    "build:cli:video": "bun build ./cmd/youtube-video-downloader.ts --compile --outfile bin/youtube-video-downloader",
    "build:cli:videos": "bun build ./cmd/youtube-videos-downloader.ts --compile --outfile bin/youtube-videos-downloader",
    "build:cli:playlist": "bun build ./cmd/youtube-playlist-downloader.ts --compile --outfile bin/youtube-playlist-downloader",
    "cli:video": "bun ./cmd/youtube-video-downloader.ts",
    "cli:videos": "bun ./cmd/youtube-videos-downloader.ts",
    "cli:playlist": "bun ./cmd/youtube-playlist-downloader.ts",
    "web:dev": "bun run --cwd ./web --hot index.ts"
  },
  "devDependencies": {
    "@types/archiver": "^7.0.0",
    "@types/bun": "^1.3.2",
    "@types/jsdom": "^27.0.0",
    "@types/ws": "^8.18.1",
    "prettier": "^3.6.2"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.933.0",
    "@hono/zod-validator": "^0.7.4",
    "@redis/client": "^5.9.0",
    "archiver": "^7.0.1",
    "bgutils-js": "^3.2.0",
    "dotenv": "^17.2.3",
    "ffmpeg-static": "^5.3.0",
    "googlevideo": "^4.0.4",
    "hono": "4.10.6",
    "jsdom": "^27.2.0",
    "youtubei.js": "^16.0.1",
    "zod": "^4.1.12"
  }
}
